{% block body %}
<div class="navbar-container">
    <div class="navbar">
        <ul>
            {% if is_granted('ROLE_USER') %}
                <li><a href="{{ path('home') }}">Home</a></li>
                <li><a href={{ path('logout') }}>Logout</a></li>
            {% else %}
                <li><a href="{{ path('home') }}">Home</a></li>
                <li><a href="{{ path('register') }}">Register</a></li>
                <li><a href={{ path('login') }}>Login</a></li>
            {% endif %}
        </ul>
    </div>
</div>
{% endblock %}